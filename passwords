import json
def load_passwords(self):
    try:
        with open(self.filename, 'r') as f:
            data = json.load(f)
            if isinstance(data, dict):  # Проверяем, что данные - это словарь
                return data
            else:
                return {}  # Если это не словарь, возвращаем пустой словарь
    except FileNotFoundError:
        return {}
